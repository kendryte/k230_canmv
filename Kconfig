mainmenu "K230 CanMV Project Configuration"

choice BOARD
    bool "Board"
    default K230_EVB

    config K230_EVB
        bool "k230 evb"
    config K230_PI
        bool "k230 pi"
endchoice

config BOARD
    string
    default "k230_evb" if K230_EVB
    default "k230_pi" if K230_PI

